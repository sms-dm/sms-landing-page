{
  "rewrites": [
    {
      "source": "/demo",
      "destination": "/demo/index.html"
    },
    {
      "source": "/demo/**",
      "destination": "/demo/:splat"
    },
    {
      "source": "!/demo/**",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}