# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# AWS Configuration
VITE_AWS_REGION=us-east-1
VITE_AWS_S3_BUCKET=sms-onboarding-assets
VITE_AWS_CLOUDFRONT_URL=https://your-cloudfront-url.cloudfront.net

# API Configuration
VITE_API_BASE_URL=https://api.sms-onboarding.com
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_APP_NAME=SMS Onboarding Portal
VITE_APP_VERSION=1.0.0
VITE_ENABLE_OFFLINE=true
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp,application/pdf

# Security Configuration
VITE_JWT_EXPIRY=86400
VITE_ENCRYPTION_ALGORITHM=AES-256-GCM

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_MOCK_DATA=false

# Integration Configuration
VITE_MAINTENANCE_PORTAL_URL=https://maintenance.sms-portal.com
VITE_WEBHOOK_URL=https://api.sms-onboarding.com/webhooks